{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "paths": {
      "@models": ["./src/models"],
      "@components": ["./src/components/"],
      "@theme": ["./native-base-theme/variables/theme.js"],
      "@navigation": ["./src/navigation"]
    }
  }
}
